{
	"manifest_version": 2,
	"name": "twimg dwldr",
	"version": "0.1",
	"description": "Twitter Image Downloader",
	"icons":{},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["script.js"]
	}],
	"browser_action": {
		"default_title": "twimg dwldr"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions":[
		"tabs",
		"background",
		"https://*/*"
	]
}
